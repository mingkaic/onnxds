
load("@com_google_protobuf//:protobuf.bzl", "py_proto_library")

package(
    default_visibility = ["//visibility:public"],
)

proto_library(
    name = "dataset_pb",
    srcs = ["datasets.proto"],
    deps = ["//onnx:onnx_pb"],
)

