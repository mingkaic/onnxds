py_library(
    name = "dataset_py_pb",
    srcs = ["datasets_pb2.py"],
    deps = ["//onnx:onnx_py_pb"],
)

py_binary(
    name = "tfgen_dataset",
    srcs = ["tfgen_dataset.py"],
    deps = [":dataset_py_pb"],
)

py_library(
    name = "read_dataset",
    srcs = ["read_dataset.py"],
    deps = [":dataset_py_pb"],
)

py_test(
    name = "readtest",
    srcs = ["readtest.py"],
    deps = [":read_dataset"],
    data = [
        "models/mnist.onnx",
        "models/mnist_5batch.onnx",
    ],
)
